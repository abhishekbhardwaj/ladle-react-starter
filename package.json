{
  "name": "ladle-react-starter",
  "version": "0.0.0",
  "description": "Minimal starter package to build a React based UI component library. Uses Ladle and Typescript.",
  "repository": {
    "type": "git",
    "url": "git@github.com:abhishekbhardwaj/ladle-react-starter.git"
  },
  "author": "Abhishek Bhardwaj <abhishek66ster@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "types": "./dist/cjs/index.d.ts",
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev:commit": "cz",
    "dev:open-bundle-visualizer": "open rollup-plugin-visualizer-stats.html",
    "dev": "yarn-or-npm run ladle:dev",
    "ladle:dev": "ladle dev -p 6789",
    "ladle:build": "ladle build -o build",
    "ladle:build:run": "ladle preview -o build",
    "test:coverage": "yarn-or-npm test --coverage",
    "test:debug": "yarn-or-npm test --debug --detectOpenHandles",
    "test:ci": "yarn-or-npm test --runInBand --no-cache --ci",
    "test": "NODE_ENV=test jest",
    "build": "yarn-or-npm clean && rollup -c",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "publish-lib": "yarn-or-npm run build && yarn-or-npm publish",
    "publish-beta": "yarn-or-npm run build && yarn-or-npm publish --tag beta",
    "publish-dryrun": "yarn-or-npm run build && yarn-or-npm publish --dry-run",
    "clean": "rimraf dist rollup-plugin-visualizer-stats.html",
    "lint": "eslint './{src,test}/**/*' --ext .js,.jsx,.tsx,.ts",
    "format": "eslint './{src,test}/**/*' --ext .js,.jsx,.tsx,.ts --fix",
    "prepare": "is-ci || husky install"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.6.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.6"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@ladle/react": "^2.1.2",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-big-lib": "^8.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.9",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.5",
    "cssnano": "^5.1.12",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "open": "^8.4.0",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "rollup-plugin-visualizer": "^5.8.0",
    "semantic-release": "^19.0.3",
    "size-limit": "^8.0.1",
    "tailwindcss": "^3.1.6",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4",
    "yarn-or-npm": "^3.0.1"
  },
  "size-limit": [
    {
      "path": "dist/*.js"
    }
  ]
}
