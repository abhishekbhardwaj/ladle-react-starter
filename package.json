{
  "name": "ladle-react-starter",
  "version": "0.0.0",
  "description": "Minimal starter package to build a React based UI component library. Uses Ladle and Typescript.",
  "repository": {
    "type": "git",
    "url": "git@github.com:boilerplatedev/ladle-react-starter.git"
  },
  "author": "Abhishek Bhardwaj <abhishek66ster@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "types": "./dist/cjs/index.d.ts",
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev:commit": "cz",
    "dev:open-bundle-visualizer": "open rollup-plugin-visualizer-stats.html",
    "dev": "yarn-or-npm run ladle:dev",
    "ladle:dev": "ladle dev -p 6789",
    "ladle:build": "ladle build -o build",
    "ladle:build:run": "ladle preview -o build",
    "test:coverage": "yarn-or-npm test --coverage",
    "test:debug": "yarn-or-npm test --debug --detectOpenHandles",
    "test:ci": "yarn-or-npm test --runInBand --no-cache --ci",
    "test": "NODE_ENV=test jest",
    "build": "yarn-or-npm clean && rollup -c --bundleConfigAsCjs",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "publish-lib": "yarn-or-npm run build && yarn-or-npm publish",
    "publish-beta": "yarn-or-npm run build && yarn-or-npm publish --tag beta",
    "publish-dryrun": "yarn-or-npm run build && yarn-or-npm publish --dry-run",
    "clean": "rimraf dist rollup-plugin-visualizer-stats.html",
    "lint": "eslint './{src,test}/**/*' --ext .js,.jsx,.tsx,.ts",
    "format": "eslint './{src,test}/**/*' --ext .js,.jsx,.tsx,.ts --fix",
    "prepare": "is-ci || husky install"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.6.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.6"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@ladle/react": "^2.4.5",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-big-lib": "^8.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.2.5",
    "cssnano": "^5.1.14",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "open": "^8.4.0",
    "postcss": "^8.4.19",
    "postcss-import": "^15.0.0",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.4.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "semantic-release": "^19.0.5",
    "size-limit": "^8.1.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "yarn-or-npm": "^3.0.1"
  },
  "size-limit": [
    {
      "path": "dist/*.js"
    }
  ]
}
